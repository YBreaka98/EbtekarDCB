{
    "name": "ybreaka98/ebtekardcb",
    "description": "ebtekar dcb payment package",
    "keywords": [
        "ybreaka98",
        "laravel",
        "ebtekardcb"
    ],
    "homepage": "https://github.com/ybreaka98/ebtekardcb",
    "license": "MIT",
    "authors": [
        {
            "name": "ybreaka98",
            "email": "yousefbreaka@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "spatie/laravel-package-tools": "^1.16.0",
        "illuminate/contracts": "^11.0||^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.17",
        "nunomaduro/collision": "^8.0",
        "larastan/larastan": "^2.9",
        "orchestra/testbench": "^9.0||^10.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-arch": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan-deprecation-rules": "^1.2",
        "phpstan/phpstan-phpunit": "^1.4",
        "spatie/laravel-ray": "^1.37"
    },
    "autoload": {
        "psr-4": {
            "Ybreaka98\\EbtekarDCB\\": "src",
            "Ybreaka98\\EbtekarDCB\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ybreaka98\\EbtekarDCB\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "clear": "@php vendor/bin/testbench package:purge-ebtekardcb --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": [
            "@composer run prepare",
            "@php vendor/bin/testbench workbench:build --ansi"
        ],
        "start": [
            "Composer\\Config::disableProcessTimeout",
            "@composer run build",
            "@php vendor/bin/testbench serve"
        ],
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ybreaka98\\EbtekarDCB\\EbtekarDCBServiceProvider"
            ],
            "aliases": {
                "EbtekarDCB": "Ybreaka98\\EbtekarDCB\\Facades\\EbtekarDCB"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
